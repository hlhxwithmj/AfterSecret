@model List<AfterSecret.Models.RegisterMember>

<table>
    <tr>
        <th>Id</th>
        <th>GuestName</th>
        <th>AgentCode</th>
        <th>Email</th>
        <th>Mobile</th>
    </tr>
    @for (int i = 0; i < Model.Count; i++)
    {
        <tr>
            <td>@(i+1)</td>
            <td>@Model[i].ToString()</td>
            <td>@Model[i].AgentCode</td>
            <td>@Model[i].Email</td>
            <td>@Model[i].Mobile</td>
        </tr>
    }
</table>